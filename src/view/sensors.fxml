<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Button?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controller.Sensors"
            stylesheets="@sensors.css"
            prefHeight="200"
            prefWidth="200">
    <GridPane AnchorPane.topAnchor="10" fx:id="grid"/>
    <Button fx:id="dismiss" onMouseClicked="#dismiss" AnchorPane.bottomAnchor="45" AnchorPane.leftAnchor="15">Dismiss Warnings</Button>


</AnchorPane>
